{
  "name": "react-typescript-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "origianl:start": "react-scripts start",
    "original:build": "react-scripts build",
    "original:test": "react-scripts test",
    "original:eject": "react-scripts eject",
    "start": "env-cmd -f .env.local craco start",
    "build": "env-cmd -f .env.production craco build",
    "build:development": "env-cmd -f .env.development craco build",
    "build:stage": "env-cmd -f .env.stage craco build",
    "build:production": "env-cmd -f .env.production craco build",
    "test": "craco test"
  },
  "dependencies": {
    "@craco/craco": "^5.9.0",
    "craco-alias": "^3.0.1",
    "mobx": "^6.9.0",
    "mobx-react": "^7.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "^2.1.3",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.35",
    "@types/react": "^18.2.11",
    "@types/react-dom": "^18.2.4",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
